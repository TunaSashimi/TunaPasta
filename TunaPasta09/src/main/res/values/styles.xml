<resources xmlns:android="http://schemas.android.com/apk/res/android">

    <!-- small,medium,large -->
    <!-- 小型控件样式 -->
    <!-- 按钮小型控件直角样式01 -->
    <style name="buttonStyleSelectSmall01Direct">
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/button_background_select_small01_direct</item>
        <item name="android:textColor">@drawable/button_textcolor_select_small01_direct</item>
    </style>

    <!-- 按钮小型控件圆角样式01 -->
    <style name="buttonStyleSelectSmall01Corner">
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/button_background_select_small01_corner</item>
        <item name="android:textColor">@drawable/button_textcolor_select_small01_corner</item>
    </style>

    <!-- 按钮小型控件直角样式02 -->
    <style name="buttonStyleSelectSmall02Direct">
        <item name="android:layout_height">50dp</item>
        <item name="android:background">@drawable/button_background_select_small02_direct</item>
        <item name="android:textColor">@drawable/button_textcolor_select_small02_direct</item>
    </style>

    <!-- 按钮小型控件圆角样式02 -->
    <style name="buttonStyleSelectSmall02Corner">
        <item name="android:layout_height">60dp</item>
        <item name="android:layout_width">60dp</item>
        <item name="android:background">@drawable/button_background_select_small02_corner</item>
    </style>

    <!-- 按钮小型控件直角样式03 -->
    <style name="buttonStyleSelectSmall03Direct">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:background">@drawable/button_background_select_small03_direct</item>
    </style>

    <!-- 单选框小型控件直角样式01 -->
    <style name="ckeckboxStyleSelectSmall01Direct">
        <item name="android:layout_width">44dp</item>
        <item name="android:layout_height">44dp</item>
        <item name="android:button">@drawable/checkbox_button_select_small01_direct</item>
    </style>

    <!-- 中型控件样式 -->
    <!-- 多选框中型控件圆角样式01 -->
    <style name="radiogroupStyleSelectMedium01Corner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">30dp</item>
        <item name="android:background">@drawable/radiogroup_background_select_medium01_corner</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- 多选框中型控件圆角样式01多选按钮样式01 -->
    <style name="radiobuttonStyleSelectMedium0101Corner">
        <item name="android:button">@null</item>
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@drawable/radiobutton_textcolor_select_medium0101_corner</item>
        <item name="android:background">@drawable/radiobutton_background_select_medium0101_corner</item>
        <item name="android:gravity">center</item>
        <item name="android:checked">true</item>
    </style>

    <!-- 多选框中型控件圆角样式01多选按钮样式02 -->
    <style name="radiobuttonStyleSelectMedium0102Corner">
        <item name="android:button">@null</item>
        <item name="android:layout_width">300dp</item>
        <item name="android:layout_weight">1</item>
        <item name="android:layout_height">match_parent</item>
        <item name="android:textColor">@drawable/radiobutton_textcolor_select_medium0102_corner</item>
        <item name="android:background">@drawable/radiobutton_background_select_medium0102_corner</item>
        <item name="android:gravity">center</item>
    </style>

    <!-- 多选框中型控件圆角样式02 -->
    <style name="radiogroupStyleSelectMedium02Corner">
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">50dp</item>
        <item name="android:layout_margin">10dp</item>
        <item name="android:orientation">horizontal</item>
    </style>

    <!-- 多选框中型控件圆角样式02多选按钮样式01 -->
    <style name="radiobuttonStyleSelectMedium0201Corner">
        <item name="android:button">@null</item>
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">34dp</item>
        <item name="android:background">@drawable/radiobutton_background_select_medium0201_corner</item>
        <item name="android:gravity">center</item>
        <item name="android:textColor">#999999</item>
        <item name="android:checked">true</item>
        <item name="android:layout_marginRight">10dp</item>
    </style>

    <!-- 多选框中型控件圆角样式02多选按钮样式02 -->
    <style name="radiobuttonStyleSelectMedium0202Corner">
        <item name="android:button">@null</item>
        <item name="android:layout_width">200dp</item>
        <item name="android:layout_height">34dp</item>
        <item name="android:background">@drawable/radiobutton_background_select_medium0202_corner</item>
        <item name="android:textColor">#999999</item>
        <item name="android:gravity">center</item>
        <item name="android:layout_marginLeft">10dp</item>
    </style>

    <!-- 进度条中型控件圆角样式01 -->
    <style name="progressBarStyleCountDownMedium01Corner">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/progressbar_progressdrawable_countdown_medium01_corner</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:layout_width">match_parent</item>
        <item name="android:layout_height">0.5sp</item>
        <item name="android:minHeight">20dp</item>
        <item name="android:maxHeight">20dp</item>
    </style>

    <style name="TextColor_GreyDark_18">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="TextColor_Coffer_18">
        <item name="android:textColor">#592d2e</item>
        <item name="android:textSize">18dp</item>
    </style>

    <style name="TextColor_GreyDark_22">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">22dp</item>
    </style>

    <style name="TextColor_white_12">
        <item name="android:textColor">#ffffff</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextColor_Grey_12">
        <item name="android:textColor">#666666</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="TextColor_GreyDark_15">
        <item name="android:textColor">#444444</item>
        <item name="android:textSize">15sp</item>
    </style>

    <style name="TextColor_GreyDark_12">
        <item name="android:textColor">#444444</item>
        <item name="android:textSize">12sp</item>
    </style>

    <style name="myProgressBar">
        <item name="android:indeterminateOnly">false</item>
        <item name="android:progressDrawable">@drawable/myprogressgorizontal</item>
        <item name="android:indeterminateDrawable">@android:drawable/progress_indeterminate_horizontal</item>
        <item name="android:minHeight">20dp</item>
        <item name="android:maxHeight">20dp</item>
    </style>

    <!-- 按钮小型控件直角样式04 -->
    <style name="buttonStyleSelectSmall04Direct">
        <item name="android:layout_width">60dp</item>
        <item name="android:layout_height">32dp</item>
        <item name="android:background">@drawable/button_background_select_small04_direct</item>
    </style>

</resources>