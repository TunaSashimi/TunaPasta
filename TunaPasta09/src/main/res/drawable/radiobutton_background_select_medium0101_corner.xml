<?xml version="1.0" encoding="utf-8"?>
<selector xmlns:android="http://schemas.android.com/apk/res/android">
    <item android:state_checked="true">
        <shape>
            <solid android:color="#592D2D" />
            <stroke android:width="1.0dp" android:color="#592D2D" />
            <corners android:bottomLeftRadius="6.0dp" android:topLeftRadius="6.0dp" />
        </shape>
    </item>
    <item android:state_pressed="true">
        <shape>
            <solid android:color="#AC9695" />
            <stroke android:width="1.0dp" android:color="#592D2D" />
            <corners android:bottomLeftRadius="6.0dp" android:topLeftRadius="6.0dp" />
        </shape>
    </item>
</selector>