<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="#dee4db">

    <!--    -->
    <ImageView
        android:id="@+id/image_my_family"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_table" />

    <ImageView
        android:id="@+id/img_setting"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginStart="24dp"
        android:layout_marginTop="68dp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent"
        app:srcCompat="@drawable/img_setting" />

    <ImageView
        android:id="@+id/img_message"
        android:layout_width="36dp"
        android:layout_height="36dp"
        android:layout_marginStart="16dp"
        app:layout_constraintStart_toEndOf="@id/img_setting"
        app:layout_constraintTop_toTopOf="@id/img_setting"
        app:srcCompat="@drawable/img_message" />

    <TextView
        android:id="@+id/text_register_login"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="26dp"
        android:layout_marginTop="48dp"
        android:text="注册/登录"
        android:textColor="#36444e"
        android:textSize="26sp"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@id/img_setting" />

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_member"
        android:layout_width="0dp"
        android:layout_height="60dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="26dp"
        android:background="@drawable/card_vip"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text_register_login">

        <TextView
            android:id="@+id/text_jiahui_member"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="16dp"
            android:drawableStart="@drawable/icon_vip"
            android:drawablePadding="6dp"
            android:text="嘉会会员"
            android:textColor="#ffe5b5"
            android:textSize="14sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_member_benefits"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginEnd="16dp"
            android:text="尊享会员权益"
            android:textColor="#ffe0a8"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_money"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="10dp"
        android:layout_marginEnd="16dp"
        android:background="@drawable/card_money"
        app:layout_constraintEnd_toStartOf="@id/constraint_coupons"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraint_member">

        <TextView
            android:id="@+id/text_money_tips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="健康金"
            android:textColor="#36444e"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.15" />

        <TextView
            android:id="@+id/text_money"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:text="0"
            android:textColor="#36444e"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@id/text_money_tips"
            app:layout_constraintTop_toBottomOf="@+id/text_money_tips"
            tools:layout_editor_absoluteX="24dp" />
    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_coupons"
        android:layout_width="0dp"
        android:layout_height="90dp"
        android:layout_marginEnd="26dp"
        android:background="@drawable/card_coupons"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@id/constraint_money"
        app:layout_constraintTop_toTopOf="@id/constraint_money">

        <TextView
            android:id="@+id/text_coupons_tips"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:text="优惠券(张)"
            android:textColor="#36444e"
            android:textSize="12sp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.2"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.15" />

        <TextView
            android:id="@+id/text_coupons"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="6dp"
            android:text="0"
            android:textColor="#36444e"
            android:textSize="22sp"
            android:textStyle="bold"
            app:layout_constraintStart_toStartOf="@id/text_coupons_tips"
            app:layout_constraintTop_toBottomOf="@+id/text_coupons_tips"
            tools:layout_editor_absoluteX="24dp" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_medical_service"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="26dp"
        android:background="@drawable/layout_background_ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraint_money">

        <TextView
            android:id="@+id/text_medical_service"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="16dp"
            android:text="医疗服务"
            android:textColor="#36444e"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_more"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="16dp"
            android:layout_marginEnd="20dp"
            android:drawableRight="@drawable/go"
            android:drawablePadding="6dp"
            android:text="更多"
            android:textColor="#86949E"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <TextView
            android:id="@+id/text_report"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginTop="12dp"
            android:drawableTop="@drawable/icon_report"
            android:drawablePadding="6dp"
            android:gravity="center_horizontal"
            android:text="报告查询"
            android:textColor="#36444e"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@id/text_reserve"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/text_medical_service" />

        <TextView
            android:id="@+id/text_reserve"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableTop="@drawable/icon_order"
            android:drawablePadding="6dp"
            android:gravity="center_horizontal"
            android:text="我的预约"
            android:textColor="#36444e"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@id/text_family"
            app:layout_constraintStart_toEndOf="@id/text_report"
            app:layout_constraintTop_toTopOf="@id/text_report" />

        <TextView
            android:id="@+id/text_family"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableTop="@drawable/icon_family"
            android:drawablePadding="6dp"
            android:gravity="center_horizontal"
            android:text="我的家人"
            android:textColor="#36444e"
            android:textSize="12sp"
            app:layout_constraintEnd_toStartOf="@id/text_evaluation"
            app:layout_constraintStart_toEndOf="@id/text_reserve"
            app:layout_constraintTop_toTopOf="@id/text_report" />

        <TextView
            android:id="@+id/text_evaluation"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:drawableTop="@drawable/icon_evaluation"
            android:drawablePadding="6dp"
            android:gravity="center_horizontal"
            android:text="我的评价"
            android:textColor="#36444e"
            android:textSize="12sp"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/text_family"
            app:layout_constraintTop_toTopOf="@id/text_report" />

    </androidx.constraintlayout.widget.ConstraintLayout>

    <androidx.constraintlayout.widget.ConstraintLayout
        android:id="@+id/constraint_order_service"
        android:layout_width="0dp"
        android:layout_height="130dp"
        android:layout_marginStart="26dp"
        android:layout_marginTop="16dp"
        android:layout_marginEnd="26dp"
        android:background="@drawable/layout_background_ffffff"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/constraint_medical_service">

        <TextView
            android:id="@+id/text_order_service"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="20dp"
            android:layout_marginTop="16dp"
            android:text="订单服务"
            android:textColor="#36444e"
            android:textSize="14sp"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout01"
            android:layout_width="0dp"
            android:layout_height="62dp"
            android:background="@drawable/layout_background_eef9fd"
            app:layout_constraintEnd_toStartOf="@id/constraintLayout02"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@id/text_order_service">

            <TextView
                android:id="@+id/textView8"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="32dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout02"
            android:layout_width="0dp"
            android:layout_height="62dp"
            android:background="@drawable/layout_background_edfffa"
            app:layout_constraintEnd_toStartOf="@id/constraintLayout03"
            app:layout_constraintStart_toEndOf="@id/constraintLayout01"
            app:layout_constraintTop_toTopOf="@id/constraintLayout01">

            <TextView
                android:id="@+id/textView9"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />
        </androidx.constraintlayout.widget.ConstraintLayout>

        <androidx.constraintlayout.widget.ConstraintLayout
            android:id="@+id/constraintLayout03"
            android:layout_width="0dp"
            android:layout_height="62dp"
            android:background="@drawable/layout_background_f6f5fe"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toEndOf="@id/constraintLayout02"
            app:layout_constraintTop_toTopOf="@id/constraintLayout01">

            <TextView
                android:id="@+id/textView10"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:text="TextView"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintTop_toTopOf="parent"
                tools:layout_editor_absoluteX="31dp" />
        </androidx.constraintlayout.widget.ConstraintLayout>

