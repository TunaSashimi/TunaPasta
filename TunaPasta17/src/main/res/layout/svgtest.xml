<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:orientation="vertical">

    <ImageView
        android:id="@+id/svg_android_black_24dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/svg_android_black_24dp" />

    <ImageView
        android:id="@+id/svg_arrow_upward_black_24dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/svg_arrow_upward_black_24dp" />

    <ImageView
        android:id="@+id/ic_arrow_drop_down_black_24dp"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:background="@drawable/svg_arrow_drop_down_black_24dp" />
</LinearLayout>

